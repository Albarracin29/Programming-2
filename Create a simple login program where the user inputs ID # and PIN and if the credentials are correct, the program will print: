#include<stdio.h>
#include<conio.h>
#include<string.h>

main(){
char ID[20];
char PIN[10];
printf("Enter ID#: ");
scanf("%s",ID);
printf("Enter PIN: ");
scanf("%s",PIN);
if (strcmp(ID, "2001101562") == 0 && strcmp(PIN, "121101") == 0)
printf("You have succesfully log in\n ID# 2001101562");
else printf("Invalid ID OR PIN \n");


return(0);
}
